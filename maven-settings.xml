<settings>
	<proxies>
		<proxy>
			<active>true</active>
			<protocol>http</protocol>
			<host>int-proxy.eurodata.de</host>
			<port>3128</port>
		</proxy>
		<proxy>
			<active>true</active>
			<protocol>https</protocol>
			<host>int-proxy.eurodata.de</host>
			<port>3128</port>
		</proxy>
	</proxies>
	<servers>
		<server>
			<id>eurodata</id>
			<username>${env.DEPLOY_USER}</username>
			<password>${env.DEPLOY_PASS}</password>
		</server>
	</servers>
</settings>
